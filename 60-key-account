public Boolean isKeyAccount(Account a) {
    Boolean isKeyAcc;

    if (
        a.AnnualRevenue == 600000 && a.Industry == 'Banking' 
        || a.AnnualRevenue == 800000 && a.Industry == 'Technology' 
        || a.AnnualRevenue == 2000000 && a.Industry == 'Retail' 
        || a.AnnualRevenue == 500000
    ) {
        isKeyAcc = true;
    } else {
        isKeyAcc = false;
    }

    return isKeyAcc;
}