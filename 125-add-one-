public void plusOne(List<Integer> numbers) {
    Integer size = numbers.size();

    numbers[size - 1]++;

    for (Integer i = 1; numbers[size - i] == 10; i++) {
        numbers[size - i] = 0;

        if (size - i == 0) {
            numbers.add(0, 1);

            break;
        }

        numbers[size - (i + 1)]++;
    }
}